{
  "name": "Coach Panel",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Devtools panel to visualise Coach results",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "assets/js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
     "16": "assets/images/Coach_Logo_16x16.png",
     "48": "assets/images/Coach_Logo_48x48.png",
    "128": "assets/images/Coach_Logo_128x128.png"
  }
}
